    "eslint-plugin-svelte3": "^4.0.0",
    "svelte": "^3.51.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.9",
    "tslib": "^2.3.1",
